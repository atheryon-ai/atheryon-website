{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/images/*", "/*.ico", "/*.png", "/*.svg", "/*.jpg", "/*.css", "/*.js"]
  },
  "routes": [
    {
      "route": "/how-we-work",
      "rewrite": "/how-we-work.html"
    },
    {
      "route": "/what-we-deliver",
      "rewrite": "/what-we-deliver.html"
    },
    {
      "route": "/recovery-migration",
      "rewrite": "/recovery-migration.html"
    },
    {
      "route": "/reference-architectures",
      "rewrite": "/reference-architectures.html"
    },
    {
      "route": "/ai-ready-data",
      "rewrite": "/ai-ready-data.html"
    },
    {
      "route": "/about",
      "rewrite": "/about.html"
    },
    {
      "route": "/contact",
      "rewrite": "/contact.html"
    },
    {
      "route": "/cdm-platform",
      "rewrite": "/cdm-platform.html"
    },
    {
      "route": "/capability-enablement",
      "rewrite": "/capability-enablement.html"
    },
    {
      "route": "/m-and-a-execution",
      "rewrite": "/m-and-a-execution.html"
    },
    {
      "route": "/privacy",
      "rewrite": "/privacy.html"
    },
    {
      "route": "/terms",
      "rewrite": "/terms.html"
    }
  ],
  "globalHeaders": {
    "Cache-Control": "public, max-age=3600",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".woff2": "font/woff2",
    ".woff": "font/woff"
  }
}
